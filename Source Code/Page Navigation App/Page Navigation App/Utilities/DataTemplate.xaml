<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Page_Navigation_App.ViewModel"
                    xmlns:view="clr-namespace:Page_Navigation_App.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SongsVM}">
        <view:Songs />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VideosVM}">
        <view:Videos />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PlaylistsVM}">
        <view:Playlists />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>
    
</ResourceDictionary>